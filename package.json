{
  "name": "api-auto-details-store",
  "version": "0.0.1",
  "description": "",
  "author": "Artem Danko",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nodemon",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d src/configs/data-source.ts",
    "db:drop": "yarn typeorm:cli schema:drop",
    "db:create": "yarn typeorm:cli migration:generate",
    "db:migrate": "yarn typeorm:cli migration:run",
    "db:revert": "yarn typeorm:cli migration:revert"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^4.0.0",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cross-var": "^1.1.0",
    "dotenv": "^16.3.1",
    "express-session": "^1.17.3",
    "hogan.js": "^3.0.2",
    "module-alias": "^2.2.3",
    "nest-winston": "^1.9.3",
    "nestjs-i18n": "^10.2.6",
    "nodemailer": "^6.9.3",
    "nodemon": "^2.0.22",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.4",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.5",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-ioredis": "^2.0.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.7",
    "@types/hogan.js": "^3.0.1",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.3",
    "@types/nodemailer": "^6.4.8",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.6.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/$1"
    }
  },
  "_moduleAliases": {
    "@app": "./dist"
  }
}
